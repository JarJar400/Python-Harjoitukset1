number1 = 5
number2 = 5

if number1 == number2:
    print("number1 and number2 are equal")

if number1 > number2:
    print("number1 is greater than number2")

if number1 >= number2:
    print("number1 is greater or equal to number2")

if number1 != number2:
    print("number1 and number2 are not equal")

#Teht채v채 2
number1 = 10
number2 = 8
number3 = 10

if number1 == number2 == number3:
    print("all numbers are equal")

if number1 == number2 or number2 == number3:
    print("either number1 == number2 or number2 == number3")

if number1 > number2 and number1 > number3:
    print("number1 is greater than both number2 and number3")

if number1 > number2:
    print("number1 is greater than number2")
else:
    if number2 > number3:
        print("number2 is greater than number3")

#Teht채v채 3
name1 = "Jari"
name2 = "Seppo"
name3 = "Jari"

if name1 == name2:
    print("name1 and name2 are equal")

if name1 != name2:
    print("name1 and name2 are not equal")

if name1 == name2:
    print("name1 and name2 are equal")
else:
    if name1 == name3:
        print("name1 and name3 are equal")

if number1 == number2:
    print("number1 and number2 are equal")
else:
    if number1 == number3:
        print("number1 and number3 are equal")
